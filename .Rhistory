library(rpart)
library(rpart.plot)
dta$Q7__23 <- as.numeric(dta$Q7__23)
cont <- rpart.control(minsplit=5,maxcompete=100,xval=10,maxdepth=30,cp=0.005) #设置参数
tree.fit <- rpart(clusters ~ edu_stage+S5+age_stage+ D5+S10C+child+Q7__23+D12A,data = dta,method = "class",parms = list(split="gini"),control=cont,na.action = na.omit)
#knitr::kable(tree.fit$cptable,caption = '复杂度参数表')
rpart.plot(tree.fit)
